#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55c84f69e780 .scope module, "ram_test" "ram_test" 2 2;
 .timescale -9 -12;
v0x55c84f6bef00_0 .var "addr", 9 0;
v0x55c84f6befe0_0 .var "clk", 0 0;
v0x55c84f6bf0b0_0 .var "data_in", 31 0;
v0x55c84f6bf1b0_0 .net "data_out", 31 0, v0x55c84f6bead0_0;  1 drivers
v0x55c84f6bf280_0 .var/i "k", 31 0;
v0x55c84f6bf370_0 .var "read", 0 0;
v0x55c84f6bf410_0 .var "write", 0 0;
S_0x55c84f69e900 .scope module, "ram_1" "ram" 2 10, 3 2 0, S_0x55c84f69e780;
 .timescale -9 -12;
    .port_info 0 /INPUT 10 "addr"
    .port_info 1 /INPUT 32 "data_in"
    .port_info 2 /INPUT 1 "clk"
    .port_info 3 /INPUT 1 "rd"
    .port_info 4 /INPUT 1 "wr"
    .port_info 5 /OUTPUT 32 "data_out"
v0x55c84f69ead0_0 .net "addr", 9 0, v0x55c84f6bef00_0;  1 drivers
v0x55c84f6be950_0 .net "clk", 0 0, v0x55c84f6befe0_0;  1 drivers
v0x55c84f6bea10_0 .net "data_in", 31 0, v0x55c84f6bf0b0_0;  1 drivers
v0x55c84f6bead0_0 .var "data_out", 31 0;
v0x55c84f6bebb0 .array "mem", 0 1023, 31 0;
v0x55c84f6becc0_0 .net "rd", 0 0, v0x55c84f6bf370_0;  1 drivers
v0x55c84f6bed80_0 .net "wr", 0 0, v0x55c84f6bf410_0;  1 drivers
E_0x55c84f698220 .event posedge, v0x55c84f6be950_0;
    .scope S_0x55c84f69e900;
T_0 ;
    %wait E_0x55c84f698220;
    %load/vec4 v0x55c84f6bed80_0;
    %load/vec4 v0x55c84f6becc0_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x55c84f6bea10_0;
    %load/vec4 v0x55c84f69ead0_0;
    %pad/u 12;
    %ix/vec4 4;
    %store/vec4a v0x55c84f6bebb0, 4, 0;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55c84f69e900;
T_1 ;
    %wait E_0x55c84f698220;
    %load/vec4 v0x55c84f6becc0_0;
    %load/vec4 v0x55c84f6bed80_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55c84f69ead0_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x55c84f6bebb0, 4;
    %store/vec4 v0x55c84f6bead0_0, 0, 32;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55c84f69e780;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c84f6befe0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x55c84f69e780;
T_3 ;
    %vpi_call 2 13 "$dumpfile", "ram_test.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55c84f69e780 {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55c84f6bf280_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x55c84f6bf280_0;
    %cmpi/s 1023, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_3.1, 5;
    %load/vec4 v0x55c84f6bf280_0;
    %load/vec4 v0x55c84f6bf280_0;
    %add;
    %pushi/vec4 256, 0, 32;
    %mod/s;
    %store/vec4 v0x55c84f6bf0b0_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c84f6bf370_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c84f6bf410_0, 0, 1;
    %load/vec4 v0x55c84f6bf280_0;
    %pad/s 10;
    %store/vec4 v0x55c84f6bef00_0, 0, 10;
    %delay 20000, 0;
    %load/vec4 v0x55c84f6bf280_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55c84f6bf280_0, 0, 32;
    %jmp T_3.0;
T_3.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c84f6bf410_0, 0, 1;
    %pushi/vec4 20, 0, 32;
T_3.2 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_3.3, 5;
    %jmp/1 T_3.3, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %vpi_func 2 27 "$random" 32 {0 0 0};
    %pushi/vec4 1024, 0, 32;
    %mod/s;
    %pad/s 10;
    %store/vec4 v0x55c84f6bef00_0, 0, 10;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55c84f6bf370_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55c84f6bf410_0, 0, 1;
    %delay 20000, 0;
    %jmp T_3.2;
T_3.3 ;
    %pop/vec4 1;
    %delay 20000, 0;
    %vpi_call 2 32 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x55c84f69e780;
T_4 ;
    %delay 10000, 0;
    %load/vec4 v0x55c84f6befe0_0;
    %inv;
    %store/vec4 v0x55c84f6befe0_0, 0, 1;
    %jmp T_4;
    .thread T_4;
